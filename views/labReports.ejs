<div class="container mt-4">
    <h1 class="mb-3">Lab Reports</h1>
    <!-- Loop through and display lab report entries -->
    <% labResults.forEach((result, index) => { %>
        <div class="row">
            <div class="col-md-6">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Lab Report <%= index + 1 %></h5>
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item"><strong>Test Name or Type:</strong> <%= result.test %></li>
                            <!-- Add more fields as needed -->
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    <% }); %>
</div>