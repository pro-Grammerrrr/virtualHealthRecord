<div class="container mt-4">
    <h1 class="mb-3">Lab Reports</h1>
    <div class="table-responsive">
        <table class="table table-striped table-hover  caption-top">
            <caption>This is Previous Lab Results</caption>

            <thead class="table-info">
                <tr>
                    <th scope="col"><i class="fa-solid fa-flask-vial fa-lg"></i></th>
                    <th scope="col">Test Name</th>
                    <th scope="col">Test Result</th>
                    <th scope="col">Reference Ranges</th>
                    <th scope="col">Interpretation</th>
                    <th scope="col">Facility Name</th>
                </tr>
            </thead>
            <tbody>
                <% labResults.forEach((result, index) => { %>
                    <tr>
                        <th scope="row"><%= index + 1 %></th>
                        <td><%= result.testName %></td>
                        <td><%= result.testResult %></td>
                        <td><%= result.referenceRanges %></td>
                        <td><%= result.interpretationOrComments %></td>
                        <td><%= result.labNameOrFacility %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</div>
