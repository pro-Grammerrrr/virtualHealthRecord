<div class="container mt-4">
    <h1 class="mb-3">Medical History</h1>
    <table class="table table-bordered border">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Diagnosis</th>
                <th scope="col">Diagnosed by Dr.</th>
                <th scope="col">Severity or Stage</th>
                <th scope="col">Diagnosis Date</th>
                <th scope="col">Treatment Plan or Recommendations</th>
            </tr>
        </thead>
        <tbody>
            <% medicalHistoryEntries.forEach((entry, index) => { %>
                <tr>
                    <th scope="row"><%= index + 1 %></th>
                    <td><%= entry.condition %></td>
                    <td><%= entry.d_id[0].fName %> <%= entry.d_id[0].lName %></td>
                    <td><%= entry.stage %></td>
                    <td><%= entry.diagnosisDate %></td>
                    <td><%= entry.treatment %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
